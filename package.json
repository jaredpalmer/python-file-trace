{
  "name": "python-file-trace",
  "version": "0.2.0",
  "description": "Determine exactly which files are necessary to run a Python application",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src/python"
  ],
  "scripts": {
    "build": "bunchee",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "bun run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish"
  },
  "keywords": [
    "python",
    "trace",
    "dependencies",
    "imports",
    "bundler"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaredpalmer/python-file-trace.git"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^20.10.0",
    "bunchee": "^5.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "minimatch": "^9.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
